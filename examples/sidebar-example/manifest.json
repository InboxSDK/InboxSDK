{
  "name": "Sidebar Example Extension",
  "description": "Sidebar example!",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": ["inboxsdk.js","bacon.js", "content.js"],
      "css": ["extension.css"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "https://mail.google.com/"
  ],
  "web_accessible_resources": [
    "inboxsdk.js.map",
    "pageWorld.js",
    "pageWorld.js.map",
    "monkey.png",
    "monkey-face.jpg",
    "lion.png"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2
}
