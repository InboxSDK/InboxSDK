{
  "name": "Compose Tooltip Example Extension",
  "description": "inboxsdk example",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "js": ["inboxsdk.js","content.js"],
      "css": [
        "content.css"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/"
  ],
  "web_accessible_resources": [
    "inboxsdk.js.map",
    "monkey.png",
    "partycat.jpg"
  ],

  "manifest_version": 2
}
